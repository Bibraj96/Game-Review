<%= link_to "Login", login_path%><br>
<h1>Welcome to Game Review!</h1>
<h3>Sign up!</h3>

<%= render partial: 'layouts/errors', locals: {object: @user} %>

<%= form_for(@user) do |f| %>
  <%= f.label :email %>
  <%= f.text_field :email %>

  <%= f.label :username %>
  <%= f.text_field :username %>

  <%= f.label :password %>
  <%= f.password_field :password%>

  <%= f.submit "Sign Up!" %>
<% end %>